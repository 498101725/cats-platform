</section>\n          </section>\n\n          <section className=\"details__section\">\n            <label className=\"details__label\">Health</label>\n\n            <section className=\"details__container\">\n              <span>{health}</span>\n            </section>\n          </section>\n\n          <section className=\"details__section\">\n            <label className=\"details__label\">Gender</label>\n\n            <section className=\"details__container\">\n              <span>{gender}</span>\n            </section>\n          </section>\n\n          <section className=\"details__section\">\n            <label className=\"details__label\">City</label>\n\n            <section className=\"details__container\">\n              <span>{city}</span>\n            </section>\n          </section>\n\n          <section className=\"details__section\">\n            <label className=\"details__label\">Contact</label>\n\n            <section className=\"details__container\">\n              <span>{email}</span>\n            </section>\n          </section>\n        </div>\n      </div>\n    );\n  }\n}\nexport default CatDetails;\n",["52","53"],"/Users/xiaosage/Documents/xiaoxiao-liu/workspace/finding-cats-homes/client/src/Pages/CatAdd/CatAdd.js",["54","55"],"import axios from \"axios\";\nimport { useState } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport Left from \"../../assets/icon/left.png\";\nimport \"./CatAdd.scss\";\n\nconst origin = \"http://localhost:9001\";\n\nfunction CatAdd() {\n  const catBreeds = [\n    \"\",\n    \"Domestic Shorthair\",\n    \"American Shorthair\",\n    \"British Shorthair\",\n    \"Exotic Shorthair\",\n    \"Egyptian Mau\",\n    \"Havana\",\n    \"Japanese Bobtail\",\n    \"Himalayan\",\n    \"Javanese\",\n    \"Korat\",\n    \"LaPerm\",\n    \"Manx\",\n    \"Munchkin\",\n    \"others\",\n  ];\n\n  const personalities = [\n    \"\",\n    \"Playful\",\n    \"intelligent\",\n    \"Friendly\",\n    \"Sociable\",\n    \"Curious\",\n    \"Lazy and Sleepy\",\n    \"Shy\",\n    \"Timid\",\n    \"Brave\",\n  ];\n\n  const [data, setData] = useState({\n    name: \"\",\n    age: \"\",\n    gender: \"girl\",\n    health: \"\",\n    personality: \"\",\n    email: \"\",\n    breed: \"\",\n    city: \"\",\n  });\n\n  const [file, setFile] = useState();\n  const [image, setImage] = useState();\n\n  const inputHandler = (e) => {\n    const newData = { ...data };\n    newData[e.target.name] = e.target.value;\n    setData(newData);\n  };\n  const onEmailHandler = (e) => {\n    const email = e.target;\n    if (email.validity.typeMismatch) {\n      email.setCustomValidity(\"please input a valid e-mail address!\");\n      email.reportValidity();\n    } else {\n      email.setCustomValidity(\"\");\n    }\n  };\n\n  const submitHandler = (e) => {\n    e.preventDefault();\n\n    axios\n      .post(`${origin}/cats`, {\n        name: data.name,\n        age: data.age,\n        gender: data.gender,\n        health: data.health,\n        personality: data.personality,\n        email: data.email,\n        breed: data.breed,\n        city: data.city,\n      })\n      .then((response) => {\n        console.log(response.data);\n      });\n  };\n\n  const onUploadImage = (e) => {\n    e.preventDefault();\n    e.cancelBubble = true;\n    const data = new FormData();\n    data.append(\"image\", file);\n    axios\n      .post(`${origin}/cats/upload`, data, {\n        headers: {\n          \"Content-Type\": \"multipart/form-data\",\n        },\n      })\n      .then(({ data }) => {\n        setImage(`${origin}${data.path}`);\n      })\n      .catch((e) => {\n        console.error(\"Error\", e);\n      });\n  };\n\n  return (\n    <div className=\"container\">\n      <div className=\"add\">\n        <form onSubmit={submitHandler} className=\"add__form\">\n          <div className=\"add__pro\">{image ? <img src={image} /> : \"\"}</div>\n          <div className=\"add__profile\">\n            <input\n              type=\"file\"\n              name=\"image\"\n              className=\"add__upload\"\n              onChange={(e) => setFile(e.target.files[0])}\n              required\n            />\n            <button disabled={!file} onClick={onUploadImage}>\n              Upload\n            </button>\n          </div>\n\n          <div className=\"add__combination\">\n            <div className=\"add__one\">\n              <div className=\"add__labels\">\n                <label className=\"add__gdr\">Gender</label>\n                <div className=\"add__labelss\">\n                  <section className=\"add__radio\">\n                    <label htmlFor=\"gender\">Boy</label>\n                    <input\n                      value=\"boy\"\n                      type=\"radio\"\n                      name=\"gender\"\n                      onChange={inputHandler}\n                      checked={data.gender === \"boy\"}\n                    />\n                  </section>\n                  <section className=\"add__radio\">\n                    <label>Girl</label>\n                    <input\n                      value=\"girl\"\n                      type=\"radio\"\n                      name=\"gender\"\n                      onChange={inputHandler}\n                      checked={data.gender === \"girl\"}\n                    />\n                  </section>\n                </div>\n              </div>\n              <div className=\"add__labels\">\n                <label htmlFor=\"name\">Name</label>\n                <input\n                  type=\"text\"\n                  name=\"name\"\n                  id=\"name\"\n                  placeholder=\"Meow's Name\"\n                  className=\"add__input\"\n                  onChange={inputHandler}\n                  required\n                />\n              </div>\n\n              <div className=\"add__labels\">\n                <label htmlFor=\"age\">Age</label>\n\n                <input\n                  autoComplete=\"new-password\"\n                  type=\"number\"\n                  name=\"age\"\n                  placeholder=\"please enter a number \"\n                  className=\"add__input\"\n                  onChange={inputHandler}\n                  required\n                />\n              </div>\n            </div>\n\n            <div className=\"add__one\">\n              <div className=\"add__labels\">\n                <label htmlFor=\"breed\">Breed</label>\n\n                <select\n                  className={\n                    \"add__input add__input--select \" +\n                    (data.breed === \"\" ? \"\" : \"add__input--selected\")\n                  }\n                  name=\"breed\"\n                  onChange={inputHandler}\n                  required\n                >\n                  {catBreeds.map((breed, index) => {\n                    return (\n                      <option key={index} value={breed}>\n                        {breed || \"Please Choose Breed\"}\n                      </option>\n                    );\n                  })}\n                </select>\n              </div>\n\n              <div className=\"add__labels\">\n                <label htmlFor=\"personality\">Personality</label>\n\n                <select\n                  className={`add__input add__input--select ${\n                    data.personality === \"\" ? \"\" : \"add__input--selected\"\n                  }`}\n                  name=\"personality\"\n                  onChange={inputHandler}\n                  required\n                >\n                  {personalities.map((personality) => {\n                    return (\n                      <option key={personality} value={personality}>\n                        {personality || \"Please Choose Personality\"}\n                      </option>\n                    );\n                  })}\n                </select>\n              </div>\n\n              <div className=\"add__labels\">\n                <label htmlFor=\"health\">Health</label>\n                <input\n                  autoComplete=\"new-password\"\n                  type=\"text\"\n                  name=\"health\"\n                  id=\"health\"\n                  placeholder=\"Health Situation \"\n                  className=\"add__input\"\n                  onChange={inputHandler}\n                  required\n                />\n              </div>\n            </div>\n\n            <div className=\"add__one add__one--feature\">\n              <div className=\"add__labels\">\n                <label htmlFor=\"city\">City</label>\n                <input\n                  autoComplete=\"new-password\"\n                  type=\"text\"\n                  name=\"city\"\n                  id=\"city\"\n                  placeholder=\"City Found \"\n                  className=\"add__input\"\n                  onChange={inputHandler}\n                  required\n                />\n              </div>\n\n              <div className=\"add__labels\">\n                <label htmlFor=\"email\">Email</label>\n                <input\n                  autoComplete=\"new-password\"\n                  type=\"email\"\n                  name=\"email\"\n                  id=\"email\"\n                  placeholder=\"me@example.com\"\n                  className=\"add__input\"\n                  onBlur={onEmailHandler}\n                  onChange={inputHandler}\n                  required\n                />\n              </div>\n\n              <div className=\"add__labels\">\n                <button type=\"submit\" className=\"add__btn\">\n                  <h2>Upload Meow</h2>\n                </button>\n              </div>\n            </div>\n          </div>\n          <Link to={`${origin}/cats`} className=\"add__link\">\n            <img src={Left} className=\"add__icon\" />\n          </Link>\n        </form>\n      </div>\n    </div>\n  );\n}\nexport default CatAdd;\n","/Users/xiaosage/Documents/xiaoxiao